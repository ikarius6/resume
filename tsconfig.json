{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/componets/*": [
        "src/components/*"
      ],
      "@/lib": [
        "src/lib/*"
      ],
      "@/store": [
        "src/store/*"
      ],
      "@/i18n": [
        "src/i18n/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "extends": "astro/tsconfigs/strict"
}